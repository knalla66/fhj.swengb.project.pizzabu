            <Label id="order" fx:id="lblTable3" maxHeight="120.0" maxWidth="120.0" minHeight="120.0" minWidth="120.0" prefHeight="120.0" prefWidth="120.0" text="" GridPane.columnIndex="1" GridPane.rowIndex="2">
